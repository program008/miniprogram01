{
  "pages": [
    "pages/home/home",
    "pages/index/index",
    "pages/logs/logs",
    "pages/knowledge/knowledge",
    "pages/navigation/navigation",
    "pages/project/project",
    "pages/webview/webview",
    "pages/user/user",
    "pages/login/login",
    "pages/register/register",
    "pages/collect/collect",
    "pages/aboutwe/aboutwe",
    "pages/kdetail/kdetail",
    "pages/weui/weui",
    "pages/vantui/vantui"
  ],
  "window": {
    "navigationBarTitleText": "WeChat",
    "navigationBarBackgroundColor": "#2196f3",
    "navigationBarTextStyle": "white",
    "backgroundTextStyle": "dark"
  },
  "networkTimeout": {
    "request": 20000,
    "connectSocket": 20000,
    "uploadFile": 20000,
    "downloadFile": 20000
  },
  "style": "v2",
  "sitemapLocation": "sitemap.json",
  "tabBar": {
    "color": "#CCCCCC",
    "selectedColor": "#2196f3",
    "backgroundColor": "#ffffff",
    "borderStyle": "white",
    "list": [{
        "pagePath": "pages/home/home",
        "text": "首页",
        "iconPath": "assets/images/icon_home_pager_not_selected.png",
        "selectedIconPath": "assets/images/icon_home_pager_selected.png"
      },
      {
        "pagePath": "pages/knowledge/knowledge",
        "text": "知识体系",
        "iconPath": "assets/images/icon_knowledge_hierarchy_not_selected.png",
        "selectedIconPath": "assets/images/icon_knowledge_hierarchy_selected.png"
      },
      {
        "pagePath": "pages/navigation/navigation",
        "text": "导航",
        "iconPath": "assets/images/icon_navigation_not_selected.png",
        "selectedIconPath": "assets/images/icon_navigation_selected.png"
      },
      {
        "pagePath": "pages/project/project",
        "text": "项目",
        "iconPath": "assets/images/icon_project_not_selected.png",
        "selectedIconPath": "assets/images/icon_project_selected.png"
      },
      {
        "pagePath": "pages/user/user",
        "text": "我的",
        "iconPath": "assets/images/icon_user_no_selected.png",
        "selectedIconPath": "assets/images/icon_user_selected.png"
      }
    ]
  },
  "plugins": {
    "weather": {
      "version": "latest",
      "provider": "wxc3db312ddf9bcb01"
    },
    "hello-plugin": {
      "version": "latest",
      "provider": "wxb74cc18049128469"
    }
  },
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  },
  "usingComponents": {
    "van-button": "miniprogram/miniprogram_npm/@vant/weapp/button/index",
    "van-cell": "miniprogram/miniprogram_npm/@vant/weapp/cell/index",
    "van-cell-group": "miniprogram/miniprogram_npm/@vant/weapp/cell-group/index",
    "van-field": "miniprogram/miniprogram_npm/@vant/weapp/field/index",
    "van-config-provider": "miniprogram/miniprogram_npm/@vant/weapp/config-provider/index",
    "van-rate":"miniprogram/miniprogram_npm/@vant/weapp/rate/index",
    "van-slider":"miniprogram/miniprogram_npm/@vant/weapp/slider/index",
    "van-icon": "miniprogram/miniprogram_npm/@vant/weapp/icon/index",
    "van-image": "miniprogram/miniprogram_npm/@vant/weapp/image/index",
    "van-row": "miniprogram/miniprogram_npm/@vant/weapp/row/index",
    "van-col": "miniprogram/miniprogram_npm/@vant/weapp/col/index",
    "van-popup": "miniprogram/miniprogram_npm/@vant/weapp/popup/index",
    "van-toast": "miniprogram/miniprogram_npm/@vant/weapp/toast/index",
    "van-calendar": "miniprogram/miniprogram_npm/@vant/weapp/calendar/index",
    "van-checkbox": "miniprogram/miniprogram_npm/@vant/weapp/checkbox/index",
    "van-checkbox-group": "miniprogram/miniprogram_npm/@vant/weapp/checkbox-group/index",
